{
    "name": "cachemaster/cache-loader",
    "version": "v1.1.3",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "description": "CacheMaster loads translations from the database and stores them in a cache for later use",
    "type": "package",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "CacheLoader\\CacheLoaderBundle\\": "src/"
        }
    },
    "keywords": [
        "framework",
        "CacheLoader",
        "load cache"
    ],
    "authors": [
        {
            "name": "KHELIFI Walid",
            "email": "khelifiwaleed@gmail.com",
            "homepage": "https://github.com/khelifiwaleed"
        }
    ],
    "require": {
        "php": ">=8.2",
        "ext-json": "*",
        "symfony/twig-bundle": "6.4.*",
        "twig/twig": "^3.0",
        "doctrine/dbal": "^3.0",
        "doctrine/doctrine-bundle": "^2.5",
        "doctrine/orm": "^2.9"
    },
    "require-dev": {
        "symfony/maker-bundle": "*"
    },
    "autoload-dev": {
        "psr-4": {
            "CacheLoader\\CacheLoaderBundle\\Tests\\": "Tests/"
        }
    }
}
